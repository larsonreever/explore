    {
      "cell_type": "code",
      "source": [
        "def classifyKeyword(keyword): \n",
        "  response = openai.Completion.create(\n",
        "    model=\"text-davinci-003\",\n",
        "    prompt=promt + kclStr + \"\\n\\nKeyword:\"+keyword+\", Classification:\",  \n",
        "    temperature=0,\n",
        "    max_tokens=1000,\n",
        "    top_p=1,\n",
        "    best_of=4,\n",
        "    frequency_penalty=0,\n",
        "    presence_penalty=0,\n",
        "    stop=[\"\\n\"]\n",
        "  )\n",
        "  return response['choices'][0]['text']"
      ],
      "metadata": {
        "id": "01_quR2ZV8Gu"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def clusterKeyword(df):\n",
        "  corpus=df['Keyword'].to_numpy().tolist()\n",
        "  response = openai.Embedding.create(\n",
        "    input=corpus,\n",
        "    model=\"text-similarity-davinci-001\"\n",
        "  )\n",
        "\n",
        "  corpus_embeddings = [ d['embedding'] for d in response['data']]\n",
        "  # Normalize the embeddings to unit length\n",
        "  corpus_embeddings = corpus_embeddings /  np.linalg.norm(corpus_embeddings, axis=1, keepdims=True)\n",
        "\n",
        "  clustering_model = KMeans()\n",
        "  clustering_model.fit(corpus_embeddings)\n",
        "  cluster_assignment = clustering_model.labels_\n",
        "  df['Cluster'] = cluster_assignment\n",
        "\n",
        "  dfPivot = pd.pivot_table(df,index=['Cluster'], values=['Keyword'], aggfunc=lambda x: x if len(x)==1 else ' '.join(x))\n",
        "  dfPivot.reset_index(inplace=True, col_level=1)\n",
        "\n",
        "  return [df, dfPivot]"
      ],
      "metadata": {
        "id": "OPOdY7KG83fV"
      },
      "execution_count": null,
      "outputs": []
    },
