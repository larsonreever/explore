        "def writeToGoogleSheet(sheetname,df):\n",
        "  try: \n",
        "    worksheetPivot = sh.worksheet(sheetname)\n",
        "    worksheetPivot.clear()\n",
        "    set_with_dataframe(worksheetPivot, df)\n",
        "  except:\n",
        "    print(\"Error \"+sheetname)"
      ],
      "metadata": {
        "id": "aQabc8zCLEKK"
      },
      "execution_count": null,
      "outputs": []
